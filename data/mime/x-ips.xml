<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/ips-script">
    <sub-class-of type="text/plain"/>
    <comment>Listaller package recipe</comment>
    <comment xml:lang="de">Setup-Script</comment>
    <glob pattern="*.ips"/>
    <icon kde="ips-script"/>
    <magic priority="50">
      <match type="string" value="IPK-Standard-Version: " offset="0"/>
    </magic>
    <supported-by>
      <application name="licreator"/>
    </supported-by>
    <expanded-acronym>Installation package source</expanded-acronym>
  </mime-type>
</mime-info>



    
