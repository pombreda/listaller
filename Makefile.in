# Makefile for Listaller 0.5
all:
	$(MAKE) -C ./build all

install:
	$(MAKE) -C ./build install

test:
	$(MAKE) -C ./build test

test-verbose:
	$(MAKE) -C ./build test ARGS=-V

translations:
	$(MAKE) -C ./build translations

i18n-update:
	$(MAKE) -C ./build i18n-update

i18n-maintain:
	$(MAKE) -C ./build i18n-maintain

clean:
	$(MAKE) -C ./build clean

distclean:
	$(MAKE) -C ./build clean
	bash ./tools/scripts/treeclean.sh

uninstall:
	$(MAKE) -C ./build uninstall
