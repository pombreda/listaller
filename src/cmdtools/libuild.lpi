<?xml version="1.0"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="8"/>
    <General>
      <SessionStorage Value="InProjectDir"/>
      <MainUnit Value="0"/>
      <TargetFileExt Value=""/>
      <UseAppBundle Value="False"/>
      <ResourceType Value="res"/>
      <UseXPManifest Value="True"/>
    </General>
    <VersionInfo>
      <UseVersionInfo Value="True"/>
      <AutoIncrementBuild Value="True"/>
      <MinorVersionNr Value="4"/>
      <StringTable Comments="" CompanyName="" FileDescription="" FileVersion="0.4.0.0" InternalName="" LegalCopyright="" LegalTrademarks="" OriginalFilename="libuild" ProductName="Listaller" ProductVersion="0.4"/>
    </VersionInfo>
    <PublishOptions>
      <Version Value="2"/>
      <IgnoreBinaries Value="False"/>
      <IncludeFileFilter Value="*.(pas|pp|inc|lfm|lpr|lrs|lpi|lpk|sh|xml)"/>
      <ExcludeFileFilter Value="*.(bak|ppu|ppw|o|so);*~;backup"/>
    </PublishOptions>
    <RunParams>
      <local>
        <FormatVersion Value="1"/>
        <LaunchingApplication Use="True" PathPlusParams="/usr/bin/xterm -T 'Lazarus Run Output' -e $(LazarusDir)/tools/runwait.sh $(TargetCmdLine)"/>
      </local>
    </RunParams>
    <Units Count="3">
      <Unit0>
        <Filename Value="libuild.lpr"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="libuild"/>
      </Unit0>
      <Unit1>
        <Filename Value="unibuild.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="unibuild"/>
      </Unit1>
      <Unit2>
        <Filename Value="ipkbuild.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="ipkbuild"/>
      </Unit2>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="9"/>
    <Target>
      <Filename Value="../../build/libuild"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)/"/>
      <Libraries Value="$(ProjOutDir)/"/>
      <OtherUnitFiles Value="../;../bind/;../../opbitmap/"/>
      <UnitOutputDirectory Value="../../build/$(TargetCPU)-$(TargetOS)"/>
      <LCLWidgetType Value="nogui"/>
    </SearchPaths>
    <Parsing>
      <SyntaxOptions>
        <UseAnsiStrings Value="False"/>
      </SyntaxOptions>
    </Parsing>
    <Other>
      <CompilerMessages>
        <UseMsgFile Value="True"/>
      </CompilerMessages>
      <CustomOptions Value="-dOpbCompat
-dNoGUI
"/>
      <CompilerPath Value="$(CompPath)"/>
    </Other>
  </CompilerOptions>
  <Debugging>
    <Exceptions Count="3">
      <Item1>
        <Name Value="EAbort"/>
      </Item1>
      <Item2>
        <Name Value="ECodetoolError"/>
      </Item2>
      <Item3>
        <Name Value="EFOpenError"/>
      </Item3>
    </Exceptions>
  </Debugging>
</CONFIG>
